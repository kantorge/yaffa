{
    "name": "kantorge/yaffa",
    "description": "Yet Another Free Financial Application: a free, self-hosted personal finance app",
    "version": "2.0.0",
    "type": "project",
    "keywords": [
        "personal finance",
        "laravel",
        "yaffa"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "beyondcode/laravel-mailbox": "^5.0",
        "biscolab/laravel-recaptcha": "dev-maintenance",
        "bkwld/cloner": "^3.14",
        "diglactic/laravel-breadcrumbs": "^10.0",
        "guzzlehttp/guzzle": "^7.10",
        "kantorge/laravel-currency-exchange-rates": "^0.0.3",
        "laracasts/utilities": "^3.2",
        "laravel/framework": "^12.43",
        "laravel/sanctum": "^4.2",
        "laravel/telescope": "^5.14",
        "laravel/ui": "^4.6",
        "openai-php/laravel": "^0.17",
        "riverskies/laravel-mobile-detect": "^1.3",
        "roach-php/laravel": "^3.2",
        "sentry/sentry-laravel": "^4.18",
        "simshaun/recurr": "^5.0",
        "spatie/laravel-model-flags": "^1.4",
        "spatie/laravel-onboard": "^2.6",
        "symfony/http-client": "^7.2",
        "tightenco/ziggy": "^2.6"
    },
    "require-dev": {
        "alebatistella/duskapiconf": "^1.2",
        "barryvdh/laravel-debugbar": "^3.16",
        "barryvdh/laravel-ide-helper": "^3.6",
        "deployer/deployer": "^7.5",
        "fakerphp/faker": "^1.23",
        "imanghafoori/laravel-microscope": "^1.0",
        "kkomelin/laravel-translatable-string-exporter": "^1.23",
        "larastan/larastan": "^3.1",
        "laravel/boost": "^1.6",
        "laravel/dusk": "^8.3",
        "laravel/pint": "^1.25",
        "laravel/sail": "^1.41",
        "laravel/tinker": "^2.10.1",
        "mockery/mockery": "^1.6",
        "nikaia/translation-sheet": "dev-feature/config_empty_behavior",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3",
        "roquie/laravel-dusk-select2": "dev-feature/search_selector_delay",
        "ryoluo/sail-ssl": "^1.4",
        "spatie/db-dumper": "^3.8"
    },
    "config": {
        "discard-changes": true,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan boost:update --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kantorge/translation-sheet"
        },
        {
            "type": "vcs",
            "url": "https://github.com/kantorge/laravel-dusk-select2"
        },
        {
            "type": "vcs",
            "url": "https://github.com/kantorge/laravel-recaptcha"
        }
    ],
    "minimum-stability": "stable"
}

{
    "name": "kantorge/yaffa",
    "type": "project",
    "description": "Yet Another Free Financial Application",
    "keywords": [
        "personal finance",
        "laravel",
        "yaffa"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "amrshawky/laravel-currency": "^5.0",
        "enlightn/enlightn": "^2.1",
        "fakerphp/faker": "^1.13",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laracasts/utilities": "^3.2",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.12",
        "laravel/ui": "^3.3",
        "nesbot/carbon": "^2.40",
        "simshaun/recurr": "^4.0",
        "tightenco/ziggy": "^1.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "imanghafoori/laravel-microscope": "^1.0",
        "kkomelin/laravel-translatable-string-exporter": "^1.17",
        "laravel/pint": "^1.1",
        "laravel/telescope": "^4.6",
        "laravel/tinker": "^2.5",
        "mockery/mockery": "^1.4.2",
        "nikaia/translation-sheet": "dev-feature/config_empty_behavior",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.0",
        "nunomaduro/phpinsights": "^2.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ignition": "^1.0"
    },
    "config": {
        "discard-changes": true,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kantorge/translation-sheet"
        }
    ]
}
